{
    "name": "johnylemon/laravel-apidocs",
    "type": "library",
    "description": "Laravel API documentation generating tool",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Krzempek",
            "email": "hello@johnylemon.dev",
            "homepage": "https://johnylemon.dev"
        }
    ],
    "keywords": [
        "laravel",
        "swagger",
        "api",
        "documentation",
        "apidocs",
        "openapi",
        "docs",
        "generate"
    ],
    "require": {
        "laravel/framework": "^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Johnylemon\\Apidocs\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Johnylemon\\Apidocs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Johnylemon\\Apidocs\\Providers\\ApidocsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
